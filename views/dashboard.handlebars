<div clas='container' style='width: 80vw; margin: auto;'>
  â€‹
  <!-- Headline -->
  <div class='row' style='margin-bottom: 40px'>
    <div class='col-12'>
      <p class='d-board-head' style='text-align:center;'>
        My Team
      </p>
    </div>
  </div>

  <h2>{{team.name}}</h2>
  <table class='table'>
    <thead class='table-dark'>
      <tr>
        <th scope='col'>Name</th>
        <th scope='col'>Points</th>
        <th scope='col'>Rebounds</th>
        <th scope='col'>Assists</th>
        <th scope='col'>Steals</th>
        <th scope='col'>Fantasy Points</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope='row'>{{team.player_one_info.name}}</th>
        <th>{{team.player_one_info.points_scored}}</th>
        <th>{{team.player_one_info.rebounds}}</th>
        <th>{{team.player_one_info.assists}}</th>
        <th>{{team.player_one_info.steals}}</th>
        <th>{{team.player_one_info.fantasy_points}}</th>
      </tr>
      <tr>
        <th scope='row'>{{team.player_two_info.name}}</th>
        <th>{{team.player_two_info.points_scored}}</th>
        <th>{{team.player_two_info.rebounds}}</th>
        <th>{{team.player_two_info.assists}}</th>
        <th>{{team.player_two_info.steals}}</th>
        <th>{{team.player_two_info.fantasy_points}}</th>
      </tr>
      <tr>
        <th scope='row'>{{team.player_three_info.name}}</th>
        <th>{{team.player_three_info.points_scored}}</th>
        <th>{{team.player_three_info.rebounds}}</th>
        <th>{{team.player_three_info.assists}}</th>
        <th>{{team.player_three_info.steals}}</th>
        <th>{{team.player_three_info.fantasy_points}}</th>
      </tr>
    </tbody>
  </table>

  <div
    style='display: flex; flex-direction: column; justify-content: center; align-items: center; font-size: 30px; margin-top: 50px; '
  >

    <p class='head-over'>
      Head over to Results page to see how your team stacked up!
    </p>
  </div>

</div>